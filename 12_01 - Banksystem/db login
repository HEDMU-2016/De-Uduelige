CREATE TABLE login
(
id int,
brugernavn VARCHAR(50) NOT NULL PRIMARY KEY,
adgangskode VARCHAR(50) NOT NULL,
)

CREATE TABLE konto
(
ejer varchar(255),
saldo double not null,
kontoid int identity,
startdato date not null,
slutdato date not null
)
alter table konto 
add kontoID int identity

CREATE TABLE kunde
(
navn varchar(255),
ID int identity,
email varchar(255),
startdato date,
slutdato date
)
alter table kunde add brugernavn varchar(255)
update kunde set brugernavn='dit brugernavn' where navn='jeres navn'

create table fastoverførsel
(
startdato Date not null,
sender int not null,
modtager int not null,
beløb double not null,
id int not null,
unikid int identity
)
create table postering(
modtager int not null,
sender int not null,
sendt Date not null,
beløb double not null,
)
create table ændring(
indførelsesdato date,
statement varchar(255)
)


create table timer(
tid varchar(255),
id int not null
)
insert into timer System.currenttimemilis (få værdien fra java og copy paste den ind)
og id=1.
